{
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:partykit\" \"npm run dev:hono\" --kill-others --prefix app",
    "build": "vite build --mode client && vite build",
    "dev:hono": "vite",
    "dev:wrangler": "wrangler dev src/index.tsx",
    "deploy": "npm run build && wrangler pages deploy dist",
    "dev:partykit": "npx partykit dev --live"
  },
  "dependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@vitejs/plugin-react": "^4.2.1",
    "hono": "^4.3.6",
    "partysocket": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@cloudflare/workers-types": "^4.20240403.0",
    "@hono/vite-cloudflare-pages": "^0.4.0",
    "@hono/vite-dev-server": "^0.12.1",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "^3.47.0"
  }
